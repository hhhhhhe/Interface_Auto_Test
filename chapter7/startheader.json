[
  {
    "description": "这是一个带header信息的post请求",
    "request": {
      "uri": "/post/with/header",
      "method": "post",
      "json": {
        "name": "小明",
        "age": "18"
      },
      "header": {
        "content-type": "application/json"
      }
    },
    "response": {
      "json": {
        "小明": "success",
        "status": "1"
      }
    }
  }
]